<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Buy_Price_5</fullName>
    <active>false</active>
    <errorConditionFormula>((ISBLANK(Volume_5__c) || ISBLANK(Buy_Price_4__c)) &amp;&amp; !ISBLANK(Buy_Price_5__c)) || 
(!ISBLANK(Buy_Price_5__c) &amp;&amp; Buy_Price_5__c &gt; Buy_Price_4__c)</errorConditionFormula>
    <errorDisplayField>Buy_Price_5__c</errorDisplayField>
    <errorMessage>Value must be less than Buy Price 4 (which must be defined, along with Volume 5 field)!</errorMessage>
</ValidationRule>
