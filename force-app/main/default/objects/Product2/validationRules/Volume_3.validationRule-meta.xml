<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Volume_3</fullName>
    <active>false</active>
    <errorConditionFormula>!ISBLANK(Volume_3__c) &amp;&amp; (ISBLANK(Volume_2__c) || Volume_3__c &lt;= Volume_2__c)</errorConditionFormula>
    <errorDisplayField>Volume_3__c</errorDisplayField>
    <errorMessage>Value must be greater than Volume 2 (and Volume 2 must be defined)!</errorMessage>
</ValidationRule>
