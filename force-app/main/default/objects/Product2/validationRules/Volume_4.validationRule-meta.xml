<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Volume_4</fullName>
    <active>false</active>
    <errorConditionFormula>!ISBLANK(Volume_4__c) &amp;&amp; (ISBLANK(Volume_3__c) || Volume_4__c &lt;= Volume_3__c)</errorConditionFormula>
    <errorDisplayField>Volume_4__c</errorDisplayField>
    <errorMessage>Value must be greater than Volume 3 (and Volume 3 must be defined)!</errorMessage>
</ValidationRule>
