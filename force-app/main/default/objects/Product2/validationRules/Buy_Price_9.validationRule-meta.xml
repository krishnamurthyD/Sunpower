<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Buy_Price_9</fullName>
    <active>false</active>
    <errorConditionFormula>((ISBLANK(Volume_9__c) || ISBLANK(Buy_Price_8__c)) &amp;&amp; !ISBLANK(Buy_Price_9__c)) || 
(!ISBLANK(Buy_Price_9__c) &amp;&amp; Buy_Price_9__c &gt; Buy_Price_8__c)</errorConditionFormula>
    <errorDisplayField>Buy_Price_9__c</errorDisplayField>
    <errorMessage>Value must be less than Buy Price 8 (which must be defined, along with Volume 9 field)!</errorMessage>
</ValidationRule>
