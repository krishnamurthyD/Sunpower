<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Volume_5</fullName>
    <active>false</active>
    <errorConditionFormula>!ISBLANK(Volume_5__c) &amp;&amp; (ISBLANK(Volume_4__c) || Volume_5__c &lt;= Volume_4__c)</errorConditionFormula>
    <errorDisplayField>Volume_5__c</errorDisplayField>
    <errorMessage>Value must be greater than Volume 4 (and Volume 4 must be defined)!</errorMessage>
</ValidationRule>
