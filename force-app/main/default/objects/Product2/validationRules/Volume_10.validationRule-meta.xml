<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Volume_10</fullName>
    <active>false</active>
    <errorConditionFormula>!ISBLANK(Volume_10__c) &amp;&amp; (ISBLANK(Volume_9__c) || Volume_10__c &lt;= Volume_9__c)</errorConditionFormula>
    <errorDisplayField>Volume_10__c</errorDisplayField>
    <errorMessage>Value must be greater than Volume 9 (and Volume 9 must be defined)!</errorMessage>
</ValidationRule>
