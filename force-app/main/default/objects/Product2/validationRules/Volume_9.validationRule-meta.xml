<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Volume_9</fullName>
    <active>false</active>
    <errorConditionFormula>!ISBLANK(Volume_9__c) &amp;&amp; (ISBLANK(Volume_8__c) || Volume_9__c &lt;= Volume_8__c)</errorConditionFormula>
    <errorDisplayField>Volume_9__c</errorDisplayField>
    <errorMessage>Value must be greater than Volume 8 (and Volume 8 must be defined)!</errorMessage>
</ValidationRule>
